{
  "name": "atom-package-deps",
  "version": "5.1.0",
  "description": "Automatically install package dependencies",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "lint": "(eslint .) && (flow check) && (prettier --list-different src/*.js)",
    "test": "apm test",
    "debug": "sb-babel-cli src -o lib",
    "watch": "npm run debug -- -w",
    "clean": "shx rm -rf lib",
    "dev": "npm run clean && NODE_ENV=development BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && NODE_ENV=production BABEL_ENV=production rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steelbrain/package-deps.git"
  },
  "keywords": [
    "atom"
  ],
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/package-deps/issues"
  },
  "homepage": "https://github.com/AtomLinter/package-deps#readme",
  "dependencies": {
    "sb-fs": "^4.0.0",
    "semver": "^7.1.0"
  },
  "devDependencies": {
    "rollup": "^2.7.6",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "shx": "^0.3.2",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "eslint-config-steelbrain": "^7.0.0",
    "flow-bin": "^0.123.0",
    "jasmine-fix": "^1.3.1",
    "sb-babel-cli": "^3.0.0",
    "@types/atom": "^1.40.2",
    "@types/jasmine": "^3.5.10",
    "@types/semver": "^7.1.0"
  }
}
